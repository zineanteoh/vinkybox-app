{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Flutter-Launch-All",
			"dependsOn": ["Flutter-Launch-iOS"]
		},
		{
			"label": "Flutter-Launch-iOS",
			"dependsOn": [
				"Firebase-Emulator",
				"Flutter-iOS-1",
				"Flutter-iOS-2"
			],
			"type": "shell"
		},
		{
			"label": "Firebase-Emulator",
			"type": "shell",
			"command": "firebase emulators:start --import=exported-dev-data --export-on-exit=exported-dev-data"
		},
		{
			"label": "Flutter-iOS-1",
			"type": "shell",
			"command": "flutter run -d 'iPhone 13'"
		},
		{
			"label": "Flutter-iOS-2",
			"type": "shell",
			"command": "flutter run -d 'second iPhone 13'"
		}
	]
}
